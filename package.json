{
  "name": "warudo-doc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3002",
    "start:en": "docusaurus start --port 3010 --locale en",
    "start:zh": "docusaurus start --port 3011 --locale zh",
    "start:all": "concurrently \"cross-env DOCUSAURUS_GENERATED_FILES_DIR_NAME=.docusaurus/dev-ver/en yarn start:en\" \"cross-env DOCUSAURUS_GENERATED_FILES_DIR_NAME=.docusaurus/dev-ver/zh yarn start:zh\"",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --port 3002",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/module-type-aliases": "^3.4.0",
    "@docusaurus/plugin-google-gtag": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@docusaurus/theme-common": "^3.4.0",
    "@docusaurus/types": "^3.1.1",
    "@easyops-cn/docusaurus-search-local": "^0.40.1",
    "@mdx-js/react": "^3.0.0",
    "@types/react": "^18.2.45",
    "clsx": "^1.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "docusaurus-plugin-image-zoom": "^1.0.1",
    "docusaurus-plugin-sass": "^0.2.5",
    "dotenv": "^16.3.1",
    "node-scp": "^0.0.22",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-twitter-embed": "^4.0.4",
    "sass": "^1.68.0"
  },
  "devDependencies": {
    "@docusaurus/tsconfig": "^3.4.0",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "docusaurus-plugin-image-zoom": {
      "@docusaurus/theme-classic": "^3.0.0"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "packageManager": "yarn@4.1.1"
}
